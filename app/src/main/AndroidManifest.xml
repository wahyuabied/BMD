<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mrabid.bumidesa">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".SplashScreenActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name=".LupaPasswordActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".PesanProfilActivity" />
        <activity
            android:name=".EditDetailProfilActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".UbahKataSandiActivity" />
        <activity android:name=".SyaratdanKetentuanActivity" />
        <activity
            android:name=".DetailProductActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".KeranjangActivity" />
        <activity
            android:name=".WhislistActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".VendorActivity" />
        <activity android:name=".TransaksiActivity" />
        <activity android:name=".AboutUsActivity" />
        <activity android:name=".FaqActivity" />
        <activity android:name=".AktivasiAkunActivity" />
        <activity android:name=".ListProductActivity"></activity>
    </application>

</manifest>